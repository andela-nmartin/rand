<p id="notice"><%= notice %></p>

<h1>Listing Timetracks</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>User</th>
      <th>Project</th>
      <th>Total time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @timetracks.each do |timetrack| %>
      <tr>
        <td><%= timetrack.date %></td>
        <td><%= timetrack.user_id %></td>
        <td><%= timetrack.project_id %></td>
        <td><%= timetrack.total_time %></td>
        <td><%= link_to 'Show', timetrack %></td>
        <td><%= link_to 'Edit', edit_timetrack_path(timetrack) %></td>
        <td><%= link_to 'Destroy', timetrack, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Timetrack', new_timetrack_path %>
